{
  "stryker-config": {
    "reporters": [
      "dots",
      "json",
      "html",
      "markdown"
    ],
    "ignore-methods": [
      "*Exception.ctor",
      "AddError",
      "ConfigureAwait",
      "Dispose",
      "LogError",
      "Debug.Assert",
      "LogInformation",
      "Guard.NotNull"
    ],
    "ignore-mutations": [
    ],
    "concurrency": 4,
    "configuration": "Debug",
    "target-framework": "net8.0",
    "thresholds": {
      "high": 90,
      "low": 70,
      "break": 60
    }
  }
}
